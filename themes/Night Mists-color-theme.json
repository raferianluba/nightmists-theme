{
  "$schema": "vscode://schemas/color-theme",
  "name": "Night Mists",
  "type": "dark",
  "settings": {
      "background": "#1b1b1e",
      "foreground": "#fbfffe",
      "editor.selectionBackground": "#fbfffe",
      "editor.selectionForeground": "#1b1b1e",
      "editor.selectionHighlightBackground": "#fbfffe",
      "editor.wordHighlightBackground": "#fbfffe",
      "editor.wordHighlightStrongBackground": "#fbfffe",
      "editor.findMatchBackground": "#fbfffe",
      "editor.findMatchHighlightBackground": "#fbfffe",
      "editor.findRangeHighlightBackground": "#fbfffe",
      "editor.hoverHighlightBackground": "#fbfffe",
      "editorLink.activeForeground": "#fbfffe",

      "editor.tokenColorCustomizations": {
          "textMateRules": [
              {
                  "scope": "entity.name.function",
                  "settings": {
                      "foreground": "#FF3F3F"
                  }
              },
              {
                  "scope": "entity.name.variable, entity.name.property",
                  "settings": {
                      "foreground": "#EFF2F1"
                  }
              },
              {
                  "scope": "entity.name.class",
                  "settings": {
                      "foreground": "#e5c07b"
                  }
              },
              {
                  "scope": "constant.numeric",
                  "settings": {
                      "foreground": "#d19a66"
                  }
              },
              {
                  "scope": "keyword",
                  "settings": {
                      "foreground": "#98c379"
                  }
              },
              {
                  "scope": "keyword.control, storage.type, support.type, variable.language, entity.name.function.variable",
                  "settings": {
                      "foreground": "#56b6c2"
                  }
              },
              {
                  "scope": "comment",
                  "settings": {
                      "foreground": "rgba(229, 229, 229, 0.35)",
                      "fontStyle": "italic"
                  }
              }
          ]
      }
  }
}